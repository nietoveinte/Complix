<?xml version="1.0" encoding="utf-8"?>
<android.support.design.widget.CoordinatorLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_gravity="center"
    android:background="#fff"
    android:orientation="vertical">

    <android.support.v4.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbarFadeDuration="0">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">


    <LinearLayout
        android:id="@+id/relX"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="10dp"
        android:layout_weight="50"
        android:orientation="horizontal"
        android:weightSum="100">

        <android.support.v7.widget.AppCompatTextView
            android:id="@+id/X_cal"
            style="@style/Widget.AppCompat.Button.Borderless"
            android:layout_width="47dp"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:background="#fff"
            android:maxLength="8"
            android:maxLines="1"
            android:text="X="
            android:textAlignment="center"
            android:textColor="#3f5c7e"
            android:textSize="20dp"
            android:theme="@style/RaisedButton" />


        <android.support.v7.widget.AppCompatTextView
            android:id="@+id/tv_X"
            style="@style/Widget.AppCompat.Button.Borderless"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="40"
            android:background="@color/colorGreyDesactivado"
            android:maxLength="15"
            android:maxLines="1"
            android:paddingLeft="0dp"
            android:textAlignment="center"
            android:textAppearance="@style/Widget.Design.TextInputLayout"
            android:textSize="15dp"
            android:theme="@style/RaisedButton" />


        <android.support.v7.widget.AppCompatTextView


            android:id="@+id/tvY"
            style="@style/Widget.AppCompat.Button.Borderless"
            android:layout_width="65dp"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:background="#fff"
            android:maxLength="8"
            android:maxLines="1"
            android:text="∡="
            android:textAlignment="center"
            android:textColor="#f00"
            android:textSize="20dp"
            android:theme="@style/RaisedButton" />

        <android.support.v7.widget.AppCompatTextView
            android:id="@+id/tv_X_polar"
            style="@style/Widget.AppCompat.Button.Borderless"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="40"
            android:background="@color/colorGreyDesactivado"
            android:maxLength="15"
            android:maxLines="1"
            android:paddingLeft="0dp"
            android:textAlignment="center"
            android:textSize="15dp"
            android:theme="@style/RaisedButton" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/relY"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/relX"
        android:layout_marginTop="10dp"
        android:orientation="horizontal"
        android:weightSum="100">


        <android.support.v7.widget.AppCompatTextView

            style="@style/Widget.AppCompat.Button.Borderless"
            android:layout_width="47dp"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:background="#fff"
            android:maxLength="8"
            android:maxLines="1"
            android:text="Y="
            android:textAlignment="center"
            android:textColor="#3f5c7e"
            android:textSize="20dp"
            android:theme="@style/RaisedButton" />


        <android.support.v7.widget.AppCompatTextView
            android:id="@+id/tv_Y"
            style="@style/Widget.AppCompat.Button.Borderless"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="40"
            android:background="@color/colorGreyDesactivado"
            android:maxLength="15"
            android:maxLines="1"
            android:paddingLeft="0dp"
            android:textAlignment="center"
            android:textAppearance="@style/Widget.Design.TextInputLayout"
            android:textSize="15dp"
            android:theme="@style/RaisedButton" />


        <android.support.v7.widget.AppCompatTextView


            android:id="@+id/tvypo"
            style="@style/Widget.AppCompat.Button.Borderless"
            android:layout_width="65dp"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:background="#fff"
            android:maxLength="8"
            android:maxLines="1"
            android:text="∡="
            android:textAlignment="center"
            android:textColor="#f00"
            android:textSize="20dp"
            android:theme="@style/RaisedButton" />

        <android.support.v7.widget.AppCompatTextView
            android:id="@+id/tv_Y_polar"
            style="@style/Widget.AppCompat.Button.Borderless"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="40"
            android:background="@color/colorGreyDesactivado"
            android:maxLength="20"
            android:maxLines="1"
            android:paddingLeft="0dp"
            android:textAlignment="center"
            android:textSize="15dp"
            android:theme="@style/RaisedButton" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/relZ"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/relY"
        android:layout_marginTop="10dp"
        android:orientation="horizontal"
        android:weightSum="100">


        <android.support.v7.widget.AppCompatTextView

            style="@style/Widget.AppCompat.Button.Borderless"
            android:layout_width="47dp"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:background="#fff"
            android:maxLength="8"
            android:maxLines="1"
            android:text="Z="
            android:textAlignment="center"
            android:textColor="#3f5c7e"
            android:textSize="20dp"
            android:theme="@style/RaisedButton" />


        <android.support.v7.widget.AppCompatTextView
            android:id="@+id/tv_Z"
            style="@style/Widget.AppCompat.Button.Borderless"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="40"
            android:background="@color/colorGreyDesactivado"
            android:maxLength="15"
            android:maxLines="1"
            android:paddingLeft="0dp"
            android:textAlignment="center"
            android:textAppearance="@style/Widget.Design.TextInputLayout"
            android:textSize="15dp"
            android:theme="@style/RaisedButton" />


        <android.support.v7.widget.AppCompatTextView


            android:id="@+id/tvwpo"
            style="@style/Widget.AppCompat.Button.Borderless"
            android:layout_width="65dp"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:background="#fff"
            android:maxLength="8"
            android:maxLines="1"
            android:text="="
            android:textAlignment="center"
            android:textColor="#f00"
            android:textSize="20dp"
            android:theme="@style/RaisedButton" />

        <android.support.v7.widget.AppCompatTextView
            android:id="@+id/tv_Z_polar"
            style="@style/Widget.AppCompat.Button.Borderless"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="40"
            android:background="@color/colorGreyDesactivado"
            android:maxLength="20"
            android:maxLines="1"
            android:paddingLeft="0dp"
            android:textAlignment="center"
            android:textSize="15dp"
            android:theme="@style/RaisedButton" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/relW"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/relZ"
        android:layout_marginTop="10dp"
        android:orientation="horizontal"
        android:weightSum="100">


        <android.support.v7.widget.AppCompatTextView

            android:id="@+id/appCompatTextView"
            style="@style/Widget.AppCompat.Button.Borderless"
            android:layout_width="47dp"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:background="#fff"
            android:maxLength="8"
            android:maxLines="1"
            android:text="W="
            android:textAlignment="center"
            android:textColor="#3f5c7e"
            android:textSize="20dp"
            android:theme="@style/RaisedButton" />


        <android.support.v7.widget.AppCompatTextView
            android:id="@+id/tv_W"
            style="@style/Widget.AppCompat.Button.Borderless"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="40"
            android:background="@color/colorGreyDesactivado"
            android:maxLength="15"
            android:maxLines="1"
            android:paddingLeft="0dp"
            android:textAlignment="center"
            android:textAppearance="@style/Widget.Design.TextInputLayout"
            android:textSize="15dp"
            android:theme="@style/RaisedButton" />


        <android.support.v7.widget.AppCompatTextView


            android:id="@+id/tvzpo1"
            style="@style/Widget.AppCompat.Button.Borderless"
            android:layout_width="65dp"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:background="#fff"
            android:maxLength="8"
            android:maxLines="1"
            android:text="="
            android:textAlignment="center"
            android:textColor="#f00"
            android:textSize="20dp"
            android:theme="@style/RaisedButton" />

        <android.support.v7.widget.AppCompatTextView
            android:id="@+id/tv_W_polar"
            style="@style/Widget.AppCompat.Button.Borderless"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="40"
            android:background="@color/colorGreyDesactivado"
            android:maxLength="20"
            android:maxLines="1"
            android:paddingLeft="0dp"
            android:textAlignment="center"
            android:textSize="15dp"
            android:theme="@style/RaisedButton" />

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/relW"
        android:layout_marginTop="5dp"
        android:orientation="horizontal"
        android:weightSum="100">

        <TextView
            style="@style/Widget.AppCompat.Button.Borderless"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:layout_weight="3"
            android:padding="5dp"
            android:text="Determinante="
            android:textAlignment="center"
            android:textColor="#950"
            android:textSize="18dp"
            android:theme="@style/RaisedButton" />

        <TextView
            android:id="@+id/determinanteG"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:layout_weight="40"
            android:padding="10dp"
            android:text=""
            android:textAlignment="center"
            android:textSize="15dp"
            android:textStyle="bold" />

    </LinearLayout>




        </LinearLayout>

   </android.support.v4.widget.NestedScrollView>

</android.support.design.widget.CoordinatorLayout>